# Nouniverse DAO Hub Environment Variables
# Copy this file to .env and fill in your actual values

# The Graph Protocol API Keys (optional but recommended for production)
VITE_GRAPH_API_KEY=your_graph_api_key_here

# Custom RPC Endpoints (optional - will use public endpoints if not provided)
VITE_ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/your_alchemy_key
VITE_ETHEREUM_RPC_URL_BACKUP=https://mainnet.infura.io/v3/your_infura_key

# IPFS Configuration
VITE_PINATA_API_KEY=your_pinata_api_key
VITE_PINATA_SECRET_KEY=your_pinata_secret_key
VITE_WEB3_STORAGE_TOKEN=your_web3_storage_token

# Analytics (optional)
VITE_ANALYTICS_ID=your_analytics_id

# Feature Flags
VITE_ENABLE_SOUND=true
VITE_ENABLE_ANIMATIONS=true
VITE_ENABLE_IPFS_UPLOAD=true

# Environment
VITE_ENVIRONMENT=production